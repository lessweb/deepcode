{
  "name": "deepcoding",
  "displayName": "Deep Code",
  "description": "Vibe coding for the latest deepseek model",
  "version": "0.0.11",
  "publisher": "vegamo",
  "repository": {
    "type": "git",
    "url": "https://github.com/lessweb/deepcode.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "resources/deepcoding_icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "deepcoding.openView",
        "title": "Open Deep Code",
        "icon": {
          "light": "resources/deepcoding_icon.svg",
          "dark": "resources/deepcoding_icon.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "deepcoding",
          "title": "Deep Code",
          "icon": "resources/deepcoding_icon.png"
        }
      ]
    },
    "views": {
      "deepcoding": [
        {
          "id": "deepcoding.chatView",
          "name": "Deep Code",
          "icon": "resources/deepcoding_icon.png",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "command": "deepcoding.openView",
          "group": "navigation@100"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "vscode:prepublish": "npm run compile"
  },
  "dependencies": {
    "ignore": "^5.3.2",
    "markdown-it": "^14.1.0",
    "openai": "^4.80.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/markdown-it": "^14.1.1",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.4.5"
  }
}
